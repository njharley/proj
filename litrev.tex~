\subsection{TEITELBAUM}
\label{sec-6-1}
\subsubsection{s.i.}
\label{sec-6-1-1}
\begin{itemize}

\item Description
\label{sec-6-1-1-1}%

\item Formula\\
\label{sec-6-1-1-2}%
$$ 
s.i.(X,Y)=\sqrt{\sum_{i=1}^{6}(x_{i}-y_{i})^{2}} 
$$
\end{itemize} % ends low level
\subsection{MORRIS}
\label{sec-6-2}
\subsubsection{SIM}
\label{sec-6-2-1}
\begin{itemize}

\item Comparison Procedure
\label{sec-6-2-1-1}%
\begin{enumerate}
\item 
\item 
\end{enumerate}

\item Formula\\
\label{sec-6-2-1-2}%
$$ 
SIM\left(X,Y\right)=\sum_{i=1}^{6}\left|x_{i}-y_{i}\right| 
$$ 
or 
$$
SIM\left(X,Y\right)=\#DV\left(ICV\left(X\right),ICV\left(Y\right)\right)
$$
\end{itemize} % ends low level
\subsubsection{K}
\label{sec-6-2-2}
\begin{itemize}

\item Formula\\
\label{sec-6-2-2-1}%
$$
k=\frac{\left(\#ICV\left(R\right)+\#ICV\left(S\right)-SIM\left(R,S\right)\right)}{2}
$$ 
Rahn writes it differently (as a funtion of X and Y) 
$$
k\left(X,Y\right)=\frac{\left(\#ICV\left(X\right)+\#ICV\left(Y\right)-SIM\left(X,Y\right)\right)}{2}
$$
\end{itemize} % ends low level
\subsubsection{ASIM}
\label{sec-6-2-3}
\begin{itemize}

\item Comparison Procedure\\
\label{sec-6-2-3-1}%
Absolute SIM
Scaled version of SIM

\item Formula\\
\label{sec-6-2-3-2}%
$$
ASIM\left(X,Y\right)=\frac{SIM\left(X,Y\right)}{\left(\#ICV\left(X\right)+\#ICV\left(Y\right)\right)}
$$
\end{itemize} % ends low level
\subsection{LORD}
\label{sec-6-3}
\subsubsection{sf}
\label{sec-6-3-1}
\begin{itemize}

\item Formula\\
\label{sec-6-3-1-1}%
$$ sf\left(X,Y\right)=\frac{\sum_{i=1}^{6}\left|x_{i}-y_{i}\right|}{2} $$
or 
$$ sf\left(X,Y\right)=\frac{\#DV\left(ICV\left(X\right),ICV\left(Y\right)\right)}{2} $$
or
$$ sf(X,Y)=frac{SIM(X,Y)}{2} $$
\end{itemize} % ends low level
\subsection{ROGERS}
\label{sec-6-4}
\subsubsection{IcVD1}
\label{sec-6-4-1}
\begin{itemize}

\item Formula\\
\label{sec-6-4-1-1}%
$$
IcVD_{1}(X,Y)=\sum_{i=1}^{6}{\left|\frac{x_{i}}{\#ICV(X)}+\frac{y_{i}}{\#ICV(Y)}\right|}
$$
\end{itemize} % ends low level
\subsubsection{IcVD2}
\label{sec-6-4-2}
\begin{itemize}

\item Formula\\
\label{sec-6-4-2-1}%
$$
IcVD_{2}(X,Y)=\sqrt{\sum{\left(\frac{x_{i}}{\sqrt{\sum{x_{i}^{2}}}}-\frac{y_{i}}{\sqrt{\sum{y_{i}^{2}}}}\right)}^{2}}
$$
\end{itemize} % ends low level
\subsubsection{COS}
\label{sec-6-4-3}
\begin{itemize}

\item Formula\\
\label{sec-6-4-3-1}%
$$
Cos\theta(X,Y)=\frac{\sum{x_i.y_i}}{\sqrt{\sum{(x_i)^2}}.\sqrt{\sum{(y_i)^2}}}
$$
\end{itemize} % ends low level
\subsection{ISAACSON}
\label{sec-6-5}
\subsubsection{AMEMB2}
\label{sec-6-5-1}
\begin{itemize}

\item Formula
\label{sec-6-5-1-1}%

\end{itemize} % ends low level
\subsubsection{IcVSIM}
\label{sec-6-5-2}
\begin{itemize}

\item Formula\\
\label{sec-6-5-2-1}%
$$ IcVSIM\left(X,Y\right)=\sigma\left(IdV\right) $$
where 
$$ IdV=[(y_{1}-x_{1})(y_{2}-x_{2})...(y_{6}-x_{6})] $$
and
$$ \sigma=\sqrt{\frac{\sum(IdV_{i}-\overline{IdV})^{2}}{6}} $$
where
$$IdV_{i}$$ is the ith term of the interval-difference vector and
$$\overline {IdV}$$ is the average (mean) of the terms in the IdV
\end{itemize} % ends low level
\subsubsection{ISIM2}
\label{sec-6-5-3}

Saled version of IcVSIM
The square root is taken of 
\subsubsection{ANGLE}
\label{sec-6-5-4}
\subsection{RAHN}
\label{sec-6-6}
\subsubsection{AK}
\label{sec-6-6-1}
\begin{itemize}

\item Formula\\
\label{sec-6-6-1-1}%
$$ ak\left(X,Y\right)=\frac{2\times k\left(X,Y\right)}{\#ICV\left(X\right)+\#ICV\left(Y\right)} $$
$$ Ak(X,Y)=1-ASIM(X,Y) $$
\end{itemize} % ends low level
\subsubsection{MEMBn}
\label{sec-6-6-2}
\begin{itemize}

\item Formula\\
\label{sec-6-6-2-1}%
$$ MEMB_{n}\left(J,X,Y\right)=EMB\left(J,X\right)+EMB\left(J,Y\right) $$
for all J such that
$$ \#J=n $$
and
$$ EMB\left(J,X\right)>0 $$ and $$ EMB\left(J,Y\right)>0 $$
so\ldots{}
$$ MEMB_{\#X}\left( X,X,Y \right)=EMB\left( X,Y \right) + 1 $$
\end{itemize} % ends low level
\subsubsection{TMEMB}
\label{sec-6-6-3}
\begin{itemize}

\item Formula\\
\label{sec-6-6-3-1}%
$$ TMEMB\left(X,Y\right)=\sum_{n=2}^{12}MEMB_{n}\left(J,X,Y\right) $$
\end{itemize} % ends low level
\subsubsection{ATMEMB}
\label{sec-6-6-4}
\begin{itemize}

\item Formula\\
\label{sec-6-6-4-1}%
$$ ATMEMB\left(X,Y\right)=\frac{TMEMB\left(X,Y\right)}{2^{\#X}+2^{\#Y}-\left(\#X+\#Y+2\right)} $$
\end{itemize} % ends low level
\subsection{LEWIN}
\label{sec-6-7}
\subsubsection{EMB}
\label{sec-6-7-1}
\subsubsection{REL}
\label{sec-6-7-2}
\begin{itemize}

\item Formula\\
\label{sec-6-7-2-1}%
$$ REL(A,B)=\frac{\sum_{i=1}^{357}\sqrt{sub(A,i).sub(B,i)}}{\sqrt{\sum_{i=1}^{357}sub(A,i).\sum_{i=1}^{357}sub(B,i)}} $$
\end{itemize} % ends low level
\subsubsection{REL2}
\label{sec-6-7-3}
\begin{itemize}

\item Formula\\
\label{sec-6-7-3-1}%
$$ REL_{2}(X,Y)\frac{2\times\sum\sqrt{(x_{i}y_{i})}}{\sqrt(\#X(\#X-1)\#Y(\#Y-1))} $$
\end{itemize} % ends low level
\subsection{CASTREN}
\label{sec-6-8}
\subsubsection{\%RELn}
\label{sec-6-8-1}
\begin{itemize}

\item Description\\
\label{sec-6-8-1-1}%
Modification of sf but using nC\%V

\item Formula\\
\label{sec-6-8-1-2}%
$$ \%REL_n(X,Y)=\frac{\sum_{i=1}^{p}|x_i-y_i|}{2} $$
where xi and yi are values in the \ref{sec-7-10}
\end{itemize} % ends low level
\subsubsection{T\%REL}
\label{sec-6-8-2}
\begin{itemize}

\item Formula\\
\label{sec-6-8-2-1}%
$$ T\%REL= \frac{\sum_{n=2}^{Max}{  \%REL_n\left( X,Y \right) }}{Max-2} $$
where
if $$ \#X=\#Y, Max=\#-1 $$ else $$ Max=min(\#X,\#Y)  $$
\end{itemize} % ends low level
\subsubsection{RECREL}
\label{sec-6-8-3}
\begin{itemize}

\item Comparison Procedure
\label{sec-6-8-3-1}%
\begin{enumerate}
\item Branches
\item Levels
\item Weighting
\item Final Value
\end{enumerate}
\end{itemize} % ends low level
